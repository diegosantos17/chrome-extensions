{
  "manifest_version": 2,
  "name": "rLogin",
  "version": "0.0.1",
  "description": "Baixar Imagens do Instagram",
  "icons": {
    "256": "rl-icon.png"
  },
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
    "default_icon": "rl-icon.png",
    "default_title": "rLogin",
    "default_popup": "popup.html"
  }
}